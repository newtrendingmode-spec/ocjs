	<script type="text/javascript">
		$("#AEDLLin").hide();
		$( "input[value='ADDITIONAL ENDORSEMENT TO DL']" ).click(function() {
			if($( "input[value='ADDITIONAL ENDORSEMENT TO DL']" ).is(":checked")){
				alert("As per our records you do not have any valid LLâ€™s. If you  want to  avail AEDL service Please select State , RTO and enter your LL number .");
				$("#AEDLLin").show();
			}else
				$("#AEDLLin").hide();
		});
	</script>


