<!-- Added By venkatesh On 16-07-2016 
Select the COV CONVERSION , automatically select the ISSUE OF PSV BADGE TO A DRIVER  -->
	<script type="text/javascript">
		$( "input[value='COV CONVERSION']" ).click(function() {
			//alert("checked");
			if($( "input[value='COV CONVERSION']" ).is(":checked")){
				var dlsize=document.getElementsByName("dlc").length;
				for(var i=0; i<dlsize; i++)
				{
					if(document.getElementsByName("dlc")[i].value=="ISSUE OF PSV BADGE TO A DRIVER")
					{
						if(!document.getElementsByName("dlc")[i].checked)
						{
							var r = confirm("COV CONVERSION Service is selected. Can you proceed to select the ISSUE OF PSV BADGE TO A DRIVER Service");
							if (r == true) 
							{
								document.getElementsByName("dlc")[i].disabled=false;
								document.getElementsByName("dlc")[i].checked=true;
							} 
						}
					}
				}
			}
		});
	</script>
	<script type="text/javascript">
		$( "input[value='COV CONVERSION']" ).click(function() {
			//alert("checked");
			if(!$( "input[value='COV CONVERSION']" ).is(":checked")){
				var dlsize=document.getElementsByName("dlc").length;
				for(var i=0; i<dlsize; i++)
				{
					if(document.getElementsByName("dlc")[i].value=="ISSUE OF PSV BADGE TO A DRIVER")
					{
						if(document.getElementsByName("dlc")[i].checked)
						{
							document.getElementsByName("dlc")[i].checked=false;
						}
					}
				}
			}
		});
	</script>
	<script type="text/javascript">
		var dlsize=document.getElementsByName("dlc").length;
		for(var i=0; i<dlsize; i++)
		{
			if(document.getElementsByName("dlc")[i].value=="CANCELLATION OF NOC")
			{
				document.getElementsByName("dlc")[i].checked=true;
				document.getElementsByName("dlc")[i].disabled="true";
				var checksSize = document.getElementsByName("dlc").length;
				for(var i=0; i<checksSize; i++)
				{	
					if(document.getElementById('preMatRenewal').value=="false" && document.getElementsByName("dlc")[i].value=="RENEWAL OF DL")
					{	
						document.getElementsByName("dlc")[i].disabled=false;
						{
							document.getElementsByName("dlc")[i].checked=true;
							document.getElementsByName("dlc")[i].disabled=false;
						}
					}
				}
			}
		}
	</script>
	<script type="text/javascript">
	/* Added By Venkatesh Y on 2-04-2018 */
		var dlsize=document.getElementsByName("dlc").length;
				for(var i=0; i<dlsize; i++)
				{
					if(document.getElementsByName("dlc")[i].value=="ISSUE OF ENDORSEMENTFREE DL")
					{
						document.getElementsByName("dlc")[i].checked=true;
						//document.getElementsByName("dlc")[i].disabled="true";
						var checksSize = document.getElementsByName("dlc").length;
						for(var i=0; i<checksSize; i++)
						{	
							if(document.getElementById('preMatRenewal').value=="false" && document.getElementsByName("dlc")[i].value=="RENEWAL OF DL")
							{	
								document.getElementsByName("dlc")[i].disabled=false;
							
								if(!document.getElementsByName("dlc")[i].checked)
								{
									document.getElementsByName("dlc")[i].checked=true;
									document.getElementsByName("dlc")[i].disabled=false;
								}
							}
						}
						/* Added By Venkatesh Y on 10-10-2017 */
					}
				}
	/* End of Adding By Venkatesh Y on 2-04-2018 */
	</script>
	<script type="text/javascript">
		$( "input[value='ISSUE INTERNATIONAL DRIVING PERMIT']" ).click(function() {
			//alert("checked");
			if($( "input[value='ISSUE INTERNATIONAL DRIVING PERMIT']" ).is(":checked")){
				var dlsize=document.getElementsByName("dlc").length;
				for(var i=0; i<dlsize; i++)
				{
					if(document.getElementsByName("dlc")[i].value=="CHANGE OF ADDRESS IN DL")
					{
						if(document.getElementById('LastSelBothRTOsareNotSame').value=="true")
						{
							if(!document.getElementsByName("dlc")[i].checked)
							{
								var r = confirm("DL Holder selects ISSUE INTERNATIONAL DRIVING PERMIT Service, DL Holders Last Endorsed RTO and selected RTO both are different. Change Of Address is mandatory.");
								if (r == true) 
								{
									document.getElementsByName("dlc")[i].disabled=true;
									document.getElementsByName("dlc")[i].checked=true;
								} 
							}
							document.getElementsByName("dlc")[i].disabled=true;
						}
					}
				}
			}
			else{
				var dlsize=document.getElementsByName("dlc").length;
				for(var i=0; i<dlsize; i++)
				{
					if(document.getElementsByName("dlc")[i].value=="CHANGE OF ADDRESS IN DL")
					{
						document.getElementsByName("dlc")[i].disabled=false;
						document.getElementsByName("dlc")[i].checked=false;
					}
				} 
			}
			/* End of Adding By Venkatesh Y on 16-01-2017 */
		});
	</script>
	<script type="text/javascript">
