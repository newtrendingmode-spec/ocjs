<script type="text/javascript">
var AEDLNotAl=document.getElementById('AEDLNotAllowd').value;
//alert("AEDLNotAl---"+AEDLNotAl);
if(AEDLNotAl=="AEDLNotAllowd"){
	if($( "input[value='ADDITIONAL ENDORSEMENT TO DL']" ).is(":checked")){
		//alert("ADDITIONAL ENDORSEMENT TO DL Checked");
		var checksSize = document.getElementsByName("dlc").length;
		//alert("checksSize--"+checksSize);
		for(var i=0; i<checksSize; i++)
		{ 
			//alert("checksSize--"+checksSize);
			if(document.getElementById('AEDLNotAllowd').value=="AEDLNotAllowd" && document.getElementsByName("dlc")[i].value=="ADDITIONAL ENDORSEMENT TO DL")
			{
				//alert("--"+document.getElementsByName("dlc")[i].value);
				if(document.getElementsByName("dlc")[i].checked)
				{
				//	alert("-In IF-"+document.getElementsByName("dlc")[i].value);
					document.getElementsByName("dlc")[i].checked=false;
				}
			}
		}
	}else{
		//alert("ADDITIONAL ENDORSEMENT TO DL Not Checked");
	}
}
</script>
