	<script type="text/javascript">
	var dlcsize=document.getElementsByName("dlc").length;
	for(var i=0; i<dlcsize; i++)
	{
		if(document.getElementsByName("dlc")[i].value=="ADDITIONAL ENDORSEMENT TO DL")
		{
			aedl="true";
			document.getElementById("showaedl").style.display="none";
		}
		if(document.getElementsByName("dlc")[i].value=="RENEWAL OF DL")
		{
			renewal="true";
			document.getElementById("showrenewal").style.display="none";
		}
		if(document.getElementsByName("dlc")[i].value=="ISSUE INTERNATIONAL DRIVING PERMIT")
		{
			idp="true";
			document.getElementById("showidp").style.display="none";
		}
	}

	if(aedl=="false")
	{
		document.getElementById("showaedl").style.display="";
	}
	if(renewal=="false")
	{
		document.getElementById("showrenewal").style.display="";
	}
	if(idp=="false")
	{
		document.getElementById("showidp").style.display="";
	}
	</script>
	
	
	
	<script type="text/javascript">
	function chkDLCheck(){
		var dlsize=document.getElementsByName("dlc").length;
		var check=0;
		for(var i=0; i<dlsize; i++)
		{
			//Added by Venkatesh Y on 13-02-2016
			//alert(document.getElementsByName("dlc")[i].value);
			//alert(document.getElementById('AEDLADDED').value);
			//document.getElementById('AEDLLin').style.display = "";
			//if(document.getElementById('AEDLADDED').value=="true" && document.getElementsByName("dlc")[i].value=="ADDITIONAL ENDORSEMENT TO DL" && (document.check.applEntLLNum.value==null ||  document.check.applEntLLNum.value==" "))
			if(document.getElementById('AEDLADDED').value=="true" && document.getElementsByName("dlc")[i].value=="ADDITIONAL ENDORSEMENT TO DL")	
			{
				//alert("ADDITIONAL ENDORSEMENT TO DL");
				if(document.getElementsByName("dlc")[i].checked){
					//alert("AEDL is Select");
					
					var state = document.check.stateCodeDLTr.value;
					if(state=="-1" || state==null)
					{
				    	//document.envform.stateCodeDLTr.style.borderColor="red";
				    	MessageHandler('UC96-E08');
			            var v=document.getElementById('s4msg').value;
						alert(v);
				    	//alert('Please select a State');
				    	return false;
				    }
					
					var rto = document.check.rtoCodeDLTr.value;
					if(rto=="-1" || rto.length==0)
				    {
				    	//document.envform.rtoCodeDLTr.style.borderColor="red";
				    	MessageHandler('UC96-E09');
			            var v=document.getElementById('s4msg').value;
						alert(v);
				    	//alert('please select a RTO');
				    	return false;
				    }
					if(document.check.applEntLLNum.value==null || document.check.applEntLLNum.value==" "){
						alert("Please Enter LL Number");	
						return false;
					}
					return true;
				}
			}
			/* else{
				alert("As per our records you do not have any valid LLâ€™s. If you  want to  avail AEDL service Please enter your LL number & select RTO code.");	
				document.getElementById('AEDLLin').style.display = "";
				return false;
			} */
			//***************End******************
			if(document.getElementById('preMatRenewal').value=="false" && document.getElementsByName("dlc")[i].value=="RENEWAL OF DL")
			{
				if(document.getElementsByName("dlc")[i].disabled)
				{
					document.getElementsByName("dlc")[i].disabled=false;
					check++;
				}
			}
			//if(document.getElementsByName("dlc")[i].value=="CHANGE OF ADDRESS IN DL")
			if(document.getElementById('COAEnable').value=="true" && document.getElementsByName("dlc")[i].value=="CHANGE OF ADDRESS IN DL")
			{
				if(document.getElementsByName("dlc")[i].disabled)
				{
					document.getElementsByName("dlc")[i].disabled=false;
					check++;
				}
			}
			if(document.getElementsByName("dlc")[i].value=="CANCELLATION OF NOC")
			{
				if(document.getElementsByName("dlc")[i].disabled)
				{
					document.getElementsByName("dlc")[i].disabled=false;
					check++;
				}
			}
			if(document.getElementsByName("dlc")[i].value=="ISSUE OF PSV BADGE TO A DRIVER")
			{
				if(document.getElementsByName("dlc")[i].disabled)
				{
					document.getElementsByName("dlc")[i].disabled=false;
					check++;
				}
			}
			if(document.getElementById('surrenderOriginalDlAtdispatch').value=="true" && document.getElementById('surrOrgDL').value=="true" && document.getElementsByName("dlc")[i].value=="ISSUE OF DUPLICATE DL"){
				if(document.getElementsByName("dlc")[i].disabled)
				{
					document.getElementsByName("dlc")[i].disabled=false;
					check++;
				}
			}
			if(document.getElementById('capturePhoroAndSigMand').value=="true" && document.getElementsByName("dlc")[i].value=="CHANGE OF PHOTO AND SIGNATURE IN DL"){
				if(document.getElementsByName("dlc")[i].disabled)
				{
					document.getElementsByName("dlc")[i].disabled=false;
					check++;
				}
			}
			if(document.getElementById('dupDLSerMand').value=="true" && document.getElementsByName("dlc")[i].value=="ISSUE OF DUPLICATE DL"){
				if(document.getElementsByName("dlc")[i].disabled)
				{
					document.getElementsByName("dlc")[i].disabled=false;
					check++;
				}
			}
			if(document.getElementById('eKYCAllowApplnameModify').value=="true" && document.getElementsByName("dlc")[i].value=="CHANGE OF NAME IN DL"){
				if(document.getElementsByName("dlc")[i].disabled)
				{
					document.getElementsByName("dlc")[i].disabled=false;
					check++;
				}
			}
			if(document.getElementById('eKYCAllowDobModify').value=="true" && document.getElementsByName("dlc")[i].value=="CHANGE OF DATE OF BIRTH IN DL"){
				if(document.getElementsByName("dlc")[i].disabled)
				{
					document.getElementsByName("dlc")[i].disabled=false;
					check++;
				}
			}
			/* if(document.getElementById('eKYCDLSerReq').value=="true" && document.getElementsByName("dlc")[i].value=="CHANGE OF NAME IN DL"){
				alert("To avail the contactless service for CHANGE OF NAME IN DL, the new name must be same as per Aadhaar Card, otherwise need to visit RTO office.");
			} */
		}
		for(var j=0; j<dlsize; j++)
		{
			if(document.getElementsByName("dlc")[j].checked){
				check++;
			}
		}
		if(check==0){
			MessageHandler('UC96-E03');
            var v=document.getElementById('s4msg').value;
			alert(v);
			//alert('Please Select at least one DL Service');
			return false;
		}
		return true;
	}
	</script>
		
